<h1>Pacienti inscrisi</h1>

<table>
  <tr>
    <th>Nume</th>
    <th>Prenume</th>
    <th>Cnp</th>
    <th>Telefon1</th>
    <th>Telefon2</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @pacients.each do |pacient| %>
  <tr>
    <td><%= pacient.nume %></td>
    <td><%= pacient.prenume %></td>
    <td><%= pacient.cnp %></td>
    <td><%= pacient.telefon1 %></td>
    <td><%= pacient.telefon2 %></td>
    <td><%= link_to 'Show', pacient %></td>
    <td><%= link_to 'Edit', edit_pacient_path(pacient) %></td>
    <td><%= link_to 'Destroy', pacient, confirm: 'Are you sure?', method: :delete %></td>
    <td><%= link_to 'Studiu', pacient %></td>
  </tr>
<% end %>
</table>
<%= will_paginate @pacients %>

