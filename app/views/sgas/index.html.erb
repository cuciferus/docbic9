<h1>Listing sgas</h1>

<table>
  <tr>
    <th>Pacient</th>
    <th>Data</th>
    <th>Modificari greutate</th>
    <th>Modificari aport alimentar</th>
    <th>Simptome gi</th>
    <th>Capacitate functionala</th>
    <th>Comorbiditatea</th>
    <th>Grasime</th>
    <th>Amiotrofie</th>
    <th>Imc</th>
    <th>Pcr</th>
    <th>Feritina</th>
    <th>Albumina</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @sgas.each do |sga| %>
  <tr>
    <td><%= sga.pacient %></td>
    <td><%= sga.data %></td>
    <td><%= sga.modificari_greutate %></td>
    <td><%= sga.modificari_aport_alimentar %></td>
    <td><%= sga.simptome_gi %></td>
    <td><%= sga.capacitate_functionala %></td>
    <td><%= sga.comorbiditatea %></td>
    <td><%= sga.grasime %></td>
    <td><%= sga.amiotrofie %></td>
    <td><%= sga.imc %></td>
    <td><%= sga.pcr %></td>
    <td><%= sga.feritina %></td>
    <td><%= sga.albumina %></td>
    <td><%= link_to 'Show', sga %></td>
    <td><%= link_to 'Edit', edit_sga_path(sga) %></td>
    <td><%= link_to 'Destroy', sga, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

